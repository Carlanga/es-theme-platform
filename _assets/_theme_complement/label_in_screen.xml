<theme>
<formatVersion>7</formatVersion>

	<view name="system">

	<image name="label" extra="true">
	<visible>false</visible>
	</image>

	<image name="label_alt" extra="true">
	<path>./../osd.png</path>
	<pos>0.5 0.57</pos>
	<size>${x.width.frame} 0</size>
	<origin>0.5 0.5</origin>
	<color>10101099</color>
	<zIndex>19</zIndex>
	<storyboard>
		<animation property="h" from="0" to="0" begin="0" duration="1800" mode="linear"/>
        <animation property="h" from="0" to="0.15" begin="1800" duration="150" mode="linear"/>
        <animation property="h" from="0.15" to="0" begin="7100" duration="150" mode="linear"/>

	</storyboard>
	</image>

	<image name="label_alt" extra="true" ifSubset="screen ratio:43">
	<pos>0.5 0.5545</pos>
	</image>
	
	<text name="label_text" extra="true">
	<fontSize>0.009</fontSize>
	<fontPath>./../roundedledboard.ttf</fontPath>
	<color>ffffff</color>
	<glowColor>000000</glowColor>
	<glowSize>2</glowSize>
	<glowOffset>1</glowOffset>		
	<x>0.49</x>
	<y>0.585</y>
	<zIndex>500</zIndex>
	</text>
	
	<text name="label_text" extra="true" ifSubset="screen ratio:43">
	<y>0.568</y>
	</text>
	
	</view>

	
</theme>